{
  "name": "Filmow Redesign",
  "description": "Redesign mais moderno do site Filmow.",
  "version": "1.0.0",

  "icons": {
    "16": "images/ext-logo.png",
    "48": "images/ext-logo.png",
    "128": "images/ext-logo.png"
  },
  "action": {
    "default_icon": {
      "16": "images/ext-logo.png",
      "48": "images/ext-logo.png",
      "128": "images/ext-logo.png"
    }
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://filmow.com/*"],
      "css": ["styles.css"]
    }
  ],
  "manifest_version": 3
}
